{
	"name": "vscode-edit-csv",
	"displayName": "vscode-edit-csv",
	"description": "extension to edit csv files",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.30.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:edit-csv.preview"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "edit-csv.edit",
				"category": "CSV",
				"title": "Edit as csv"
			}
		],
		"configuration": {
			"title": "Edit csv configuration",
			"properties": {}
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^3.1.4",
		"vscode": "^1.1.25",
		"tslint": "^5.8.0",
		"@types/node": "^8.10.25",
		"@types/mocha": "^2.2.42"
	},
	"dependencies": {
		"@types/papaparse": "^4.5.6",
		"ag-grid-community": "^20.0.0",
		"bulma": "^0.7.2",
		"bulma-extensions": "^4.0.1",
		"handsontable": "^6.2.2",
		"papaparse": "^4.6.3"
	}
}
